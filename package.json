  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --bundle --format=esm --external:express --external:http --external:fs --external:path --outfile=dist/index.js && ./create-spa-routes.sh",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "contracts:compile": "cd contracts && npm run compile",
    "contracts:test": "cd contracts && npm run test",
    "contracts:deploy": "cd contracts && npm run deploy",
    "clean": "rm -rf node_modules dist contracts/node_modules contracts/cache",
    "install:all": "npm install && cd contracts && npm install"
  },
